<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="using:$NAMESPACE$">
    <Design.PreviewWith>
        <controls:$CLASS$ />
    </Design.PreviewWith>

    <Style Selector="controls|$CLASS$">
        <!-- Set Defaults -->
        <Setter Property="Template">
            <ControlTemplate>
                <TextBlock Text="Templated Control" />
            </ControlTemplate>
        </Setter>
    </Style>
</Styles>
